{
  "name": "muyu-cms",
  "version": "1.0.0",
  "description": "一个利用webpack加载的react redux实现的后台系统",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "node -r dotenv/config --harmony bin/compile  --display-modules",
    "start": "better-npm-run development",
    "start:pro": "better-npm-run production",
    "test": "set DEBUG=* && node test/test.js",
    "deploy": "better-npm-run deploy"
  },
  "betterScripts": {
    "development": {
      "command": "npm run clean && node  -r dotenv/config --harmony server/main",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "production": {
      "command": "npm run deploy &&node -r dotenv/config server/main",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-transform-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "qixian yu <858385135@qq.com> (http://github.com/laoqian)",
  "license": "CC0-1.0",
  "dependencies": {
    "autoprefixer": "^6.3.1",
    "better-npm-run": "0.0.5",
    "body-parser": "^1.14.2",
    "connect-redis": "^3.0.2",
    "cookie-parser": "^1.4.1",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "express": "^4.13.3",
    "express-session": "^1.13.0",
    "fs-extra": "^0.26.4",
    "history": "^1.17.0",
    "immutable": "^3.7.6",
    "jquery": "^3.0.0",
    "moment": "^2.11.2",
    "mysql": "^2.10.2",
    "parseurl": "^1.3.1",
    "progress": "^1.1.8",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.0.0",
    "react-router": "^2.5.2",
    "redis": "^2.4.2",
    "redux": "^3.0.0",
    "redux-thunk": "^0.1.0",
    "watchpack": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^5.6.18",
    "babel-loader": "^5.1.4",
    "babel-plugin-react-transform": "^1.1.0",
    "css-loader": "^0.23.1",
    "expect": "^1.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "is": "^3.1.0",
    "jsdom": "^5.6.1",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "mocha": "^2.3.4",
    "node-libs-browser": "^0.5.2",
    "postcss-loader": "^0.8.0",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-hmr": "^1.0.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.2.0"
  }
}
